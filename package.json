{
  "name": "markdraw",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/jonathanjameswatson/markdraw.git",
  "author": "Jonathan Watson",
  "license": "MIT",
  "devDependencies": {
    "browser-sync": "^2.26.13",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "gulp": "^4.0.2",
    "gulp-sass": "^4.1.0",
    "node-sass": "^5.0.0"
  },
  "scripts": {
    "watch": "cross-env NODE_ENV='development' yarn gulp watch",
    "dev": "concurrently --kill-others \"dotnet watch --project Markdraw run\" \"yarn run watch\"",
    "prod": "cross-env NODE_ENV='production' yarn gulp build",
    "build": "cross-env NODE_ENV='production' yarn gulp build && dotnet publish Markdraw/Markdraw.csproj -c Release -o release --nologo"
  },
  "dependencies": {
    "bulma": "^0.9.1"
  }
}
